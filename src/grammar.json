{
  "name": "githubactionsvariables",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "variable"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "whitespace": {
      "type": "PATTERN",
      "value": "\\\\s+"
    },
    "_dot": {
      "type": "STRING",
      "value": "."
    },
    "context": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "property": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_dot"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "variable": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "${{"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "context"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "property"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}}"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

