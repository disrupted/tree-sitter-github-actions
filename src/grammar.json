{
  "name": "githubactionsvariables",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "variable"
        }
      ]
    },
    "_dot": {
      "type": "STRING",
      "value": "."
    },
    "_str": {
      "type": "PATTERN",
      "value": "[\\w\\W]+"
    },
    "string_content": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": "([^'\\\\\\r\\n]|\\\\(.|\\r?\\n))+"
        }
      }
    },
    "_number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "null"
        },
        {
          "type": "SYMBOL",
          "name": "boolean"
        },
        {
          "type": "SYMBOL",
          "name": "int"
        },
        {
          "type": "SYMBOL",
          "name": "float"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    },
    "null": {
      "type": "STRING",
      "value": "null"
    },
    "true": {
      "type": "STRING",
      "value": "true"
    },
    "false": {
      "type": "STRING",
      "value": "false"
    },
    "boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "true"
        },
        {
          "type": "SYMBOL",
          "name": "false"
        }
      ]
    },
    "int": {
      "type": "SYMBOL",
      "name": "_number"
    },
    "float": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "-"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_number"
        },
        {
          "type": "SYMBOL",
          "name": "_dot"
        },
        {
          "type": "SYMBOL",
          "name": "_number"
        }
      ]
    },
    "string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "SYMBOL",
          "name": "string_content"
        },
        {
          "type": "STRING",
          "value": "'"
        }
      ]
    },
    "operator": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "logical_group"
        },
        {
          "type": "SYMBOL",
          "name": "index"
        },
        {
          "type": "SYMBOL",
          "name": "not"
        },
        {
          "type": "SYMBOL",
          "name": "lt"
        },
        {
          "type": "SYMBOL",
          "name": "le"
        },
        {
          "type": "SYMBOL",
          "name": "gt"
        },
        {
          "type": "SYMBOL",
          "name": "ge"
        },
        {
          "type": "SYMBOL",
          "name": "eq"
        },
        {
          "type": "SYMBOL",
          "name": "ne"
        },
        {
          "type": "SYMBOL",
          "name": "and"
        },
        {
          "type": "SYMBOL",
          "name": "or"
        }
      ]
    },
    "logical_group": {
      "type": "STRING",
      "value": "( )"
    },
    "index": {
      "type": "STRING",
      "value": "[ ]"
    },
    "property_deref": {
      "type": "SYMBOL",
      "name": "_dot"
    },
    "not": {
      "type": "STRING",
      "value": "!"
    },
    "lt": {
      "type": "STRING",
      "value": "<"
    },
    "le": {
      "type": "STRING",
      "value": "<="
    },
    "gt": {
      "type": "STRING",
      "value": ">"
    },
    "ge": {
      "type": "STRING",
      "value": ">="
    },
    "eq": {
      "type": "STRING",
      "value": "=="
    },
    "ne": {
      "type": "STRING",
      "value": "!="
    },
    "and": {
      "type": "STRING",
      "value": "&&"
    },
    "or": {
      "type": "STRING",
      "value": "||"
    },
    "env_var": {
      "type": "PATTERN",
      "value": "[A-Z_]+"
    },
    "whitespace": {
      "type": "PATTERN",
      "value": "\\\\s+"
    },
    "context": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "property": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "property_deref"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "identifier"
            },
            {
              "type": "SYMBOL",
              "name": "env_var"
            }
          ]
        }
      ]
    },
    "variable": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "${{"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "type"
                },
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "context"
                    },
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "SYMBOL",
                          "name": "property"
                        },
                        {
                          "type": "BLANK"
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "operator"
                      },
                      {
                        "type": "CHOICE",
                        "members": [
                          {
                            "type": "SYMBOL",
                            "name": "type"
                          },
                          {
                            "type": "SEQ",
                            "members": [
                              {
                                "type": "SYMBOL",
                                "name": "context"
                              },
                              {
                                "type": "CHOICE",
                                "members": [
                                  {
                                    "type": "SYMBOL",
                                    "name": "property"
                                  },
                                  {
                                    "type": "BLANK"
                                  }
                                ]
                              }
                            ]
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}}"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

